package edu.neu.csye7374;

import java.util.ArrayList;
import java.util.List;

public class Calculate {

	int a;
	int b;
	List<Integer> list = new ArrayList<>();
	
	public Calculate(int a, int b) {
		this.a = a;
		this.b = b;
	}
	
    public Calculate(int ... nums) {
        for (int i : nums) {
            this.list.add(i);
        }
    }

}
