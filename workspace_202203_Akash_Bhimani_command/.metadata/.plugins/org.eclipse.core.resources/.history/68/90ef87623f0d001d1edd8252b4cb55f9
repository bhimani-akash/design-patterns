public class Calculate implements Calculator {

	int a;
	int b;
	List<Integer> list = new ArrayList<>();
	
	public Calculate(int a, int b) {
		this.a = a;
		this.b = b;
	}
	
    public Calculate(int ... nums) {
        for (int i : nums) {
            this.list.add(i);
        }
    }
    
